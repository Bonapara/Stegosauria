<div class="main-container">
  <div class="gallery-container">
    <!-- Right column -->
    <div class="gallery-column-1">
      <!-- Photo Gallery -->
      <%= cl_image_tag @gallery.photo_gallery, crop: :fill, :class => "gallery-photo"%>
    </div>
    <!-- Left column -->
    <div class="gallery-column-2">
      <div class="gallery-infos">
        <h1 class="title-1">La galerie</h1>
      </div>
      <div class="gallery-map">
        <iframe
          width="400"
          height="600"
          frameborder="0" style="border:0"
          src="https://www.google.com/maps/embed/v1/place?key=AIzaSyDy7Ksnv9eMUGY_R_tHMd-kYPuqv2WeQmE
            &q=27+rue+guersant,Paris+France" allowfullscreen>
        </iframe>
      </div>
    </div>
  </div>
</div>



