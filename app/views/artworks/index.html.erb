<%= render 'shared/artworks_navbar' %>


<div class="wrapper">
  <div class="container artworks-container">
    <div class="row">
      <% @artworks.each do |artwork| %>
        <h2> <%= artwork.name %> </h2>
        <p> <%= artwork.dimensions %> </p>
        <p> <%= artwork.artwork_domain %> </p>
        <p> <%= artwork.description %> </p>
        <p> <%= link_to "Voir l'oeuvre", artwork_path(artwork) %> </p>
      <% end %>
    </div>
  </div>
</div>
